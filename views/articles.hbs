{{> header}}

<section class="container">
  <div class="layout-2 row">
    <div class="col-8">
      {{#articles}}
      <div class="container mt-5">
        <div class="card">
          <div class="card-image">
            <img src="data:image/jpeg;base64," alt="">
          </div>
          <div class="card-content">
            <div class="blog-details">
              <div class="icon-text">
                <span class="icon"><i class="fas fa-calendar"></i></span>
                <span class="text">{{dateFormat this.createdAt}}</span>
              </div>
              {{!-- <div class="icon-text">
                <span class="icon"><i class="fas fa-user"></i></span>
                <span class="text">Author</span>
              </div>
              <div class="icon-text">
                <span class="icon"><i class="far fa-comment-dots"></i></span>
                <span class="text">22</span>
              </div>
              <div class="icon-text">
                <span class="icon"><i class="far fa-heart"></i></span>
                <span class="text">09/05/2020</span>
              </div> --}}
            </div>
            <h2 class="blog-title">{{this.title}}</h2>
            <p class="excerpt">{{this.description}}</p>
            <div class="blog-cta">
              <a href="/articles/{{this._id}}" class="button">Read more</a>
              {{!-- <button class="share-button"><i class="fas fa-share-alt"></i></button> --}}
            </div>
          </div>
        </div>
      </div>
      {{/articles}}
    </div>
    <div class="col-4">
            <aside id="sidebar" class="px-1 mt-3">
              <!-- categories -->
              <section class="categories">
                <h2 class="text-center text-dark mb-3">Categories</h2>
                <a href="" class="d-flex justify-content-between text-dark">
                  <span>Design</span>
                  <span>5</span>
                </a>
                <a href="" class="d-flex justify-content-between text-dark">
                  <span>Graphic</span>
                  <span>8</span>
                </a>
                <a href="" class="d-flex justify-content-between text-dark">
                  <span>Illustration</span>
                  <span>3</span>
                </a>
                <a href="" class="d-flex justify-content-between text-dark">
                  <span>Fashion</span>
                  <span>2</span>
                </a>
              </section>
            </aside>
      </div>
  </div>
</section>

